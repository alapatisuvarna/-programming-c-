#include<iostream>
using namespace std;
int main()
{
	int n,m,i,fact=1,sum=0;
  cin>>n;
  int temp=n;
  while(n>0)
  {
    m=n%10;
    for(i=1;i<=m;i++)
    {
      fact=fact*i;
    }
    sum=sum+fact;
    fact=1;
    n=n/10;
  }
  if(sum==temp)
  {
    cout<<temp<<" is a strong number";
  }
  else
    cout<<temp<<" is not a strong number";
}
