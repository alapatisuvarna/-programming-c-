{
  private:
  int no=1234567890;
  string name="john doe";
  int balance=5000;
  int deposit=2000;
  int withdrawal=1000;
  public:
  void getdata()
  {
    cout<<"accountholdernumber:"<<no<<endl;
    cout<<"accountholdername:"<<name<<endl;
    cout<<"balance:"<<balance<<endl;
