#include<iostream>
using namespace std;
int main()
{
  int a[]={1,2,3};
  int x=sizeof(a)/sizeof(a[0]);
  int b[]={4,5};
  int y=sizeof(b)/sizeof(b[0]);
  int z=x+y;
  int c[z];
  int i,j;
  for(i=0;i<x;i++)
    c[i]=a[i];
  for(i=0,j=x;j<z,i<y;i++,j++)
    c[j]=b[i];
  for(i=0;i<z;i++)
    cout<<c[i];
}
