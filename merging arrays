#include<iostream>
using namespace std;
int main()
{
  int a[]={1,2,3};
  int m=sizeof(a)/sizeof(a[0]);
  int b[]={4,5};
  int n=sizeof(b)/sizeof(b[0]);
  int x=m+n;
  int c[x];
  int i;
  for(i=0;i<m;i++)
  {
    c[i]=a[i];
  }
  for(i=0;i<x;i++)
  {
    c[m+i]=b[i];
  }
  for(i=0;i<x;i++)
  {
    cout<<c[i];
  }
}
