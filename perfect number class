#include<iostream>
using namespace std;
class perfectnum
{
	private:
		int n;
		int sum=0;
		public:
			void getdata()
			{
				cin>>n;
			}
			void showdata()
			{
				for(int i=1;i<=n;i++)
				{
					if(n%i==0)
					{
						sum=+i;
					}
				}
				if(sum==n)
				cout<<"perfect number:"<<n;
				else
				cout<<"not a perfect number:"<<n;
			}
}p;
int main()
{
	p.getdata();
	p .showdata();
}
