#include<iostream>
using namespace std;
int main()
{
  int n,temp,m,sum=0;
  cin>>n;
  temp=n;
  while(sum!=4&&sum!=1)
  {
    sum=0;
    while(n>0)
      m=n%10;
    sum=sum+(m*m);
    n=n/10;
  }
  n=sum;
}
if(sum==1)
{
  cout<<temp<<"is happy number";
  else
    cout<<temp<<"is not a happy number";
}
